{
  "name": "@sidetree/dashboard",
  "version": "0.3.1-unstable.13",
  "private": true,
  "scripts": {
    "predev:docker": "docker-compose -f ./docker-compose.build.yml build",
    "predev:ganache:docker": "docker-compose -f ./docker-compose.build.yml build",
    "predev:ropsten:docker": "docker-compose -f ./docker-compose.build.yml build",
    "dev:docker": "docker-compose -f ./docker-compose.run.yml --env-file .env.example:sidetree.testnet up",
    "dev:ganache:docker": "docker-compose -f ./docker-compose.run.yml --env-file .env.ganache up",
    "dev:ropsten:docker": "docker-compose -f ./docker-compose.run.yml --env-file .env.ropsten up",
    "dev": "dotenv -e .env.example:sidetree.testnet next dev",
    "dev:ganache": "dotenv -e .env.ganache next dev",
    "dev:ropsten": "dotenv -e .env.ropsten next dev",
    "test": "jest",
    "build": "next build",
    "export": "next export",
    "start": "next start -p ${PORT:=3000}",
    "lint": "next lint",
    "lint:fix": "next lint --fix"
